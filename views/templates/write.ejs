<div class="row">
  	<div class="col-lg-12">
  		<div class="form-group">
  			<input type="text" name="title" id="title" class="form-control" placeholder="제목" ng-model="title" maxlength="50" />
  		</div>
  		<div class="form-group">
  			<textarea class="form-control" rows="10" name="content" id="content" ng-keyup="contentKeyup()" ng-keydown="contentKeyDown()" ng-model="content" placeholder="내용"></textarea>
  			<div style="text-align:right">
	  			<span class="byte_count">0</span>/200 byte
	  		</div>
  		</div>
  	</div>
</div>